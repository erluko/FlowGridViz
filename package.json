{
  "name": "pcapviz",
  "version": "0.7.3",
  "description": "Packet Capture Visualizer",
  "main": "server.js",
  "config": {
    "listen_ip": "127.0.0.1",
    "port": "8080",
    "url_root": "/",
    "pcap_file": "~/Downloads/ISCX_Botnet-Training.pcap",
    "num_packets": "all",
    "services_file": "/etc/services"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "make",
    "start": "node --max-old-space-size=8192 server.js",
    "debug": "node --inspect --max-old-space-size=8192 server.js",
    "make": "make",
    "conf_nginx": "node nginx/mkconf.js > nginx/pcapviz.conf"
  },
  "dependencies": {
    "express": ">=4.16.4",
    "jsdom": ">=14.0.0",
    "lru-cache": ">=5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.gatech.edu/ekobrin3/2019-Spring-CSE6242-pcapviz.git"
  },
  "author": "Eric Kobrin <ekobrin@gatech.edu>",
  "license": "BSD-2-Clause"
}
